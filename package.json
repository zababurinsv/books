{
  "name": "vue-ssr-learning",
  "version": "1.0.0",
  "description": "Learning by doing.",
  "main": "server.js",
  "scripts": {
    "dev": "node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js --progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toBeTheLight/vue-ssr-learning.git"
  },
  "keywords": [
    "vue-ssr",
    "node"
  ],
  "author": "toBeTheLight",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.1",
    "express": "^4.16.3",
    "firebase": "^5.0.4",
    "gsap": "^2.0.0",
    "socket.io-client": "^2.1.1",
    "vue": "^2.5.16",
    "vue-meta": "^1.5.0",
    "vue-outside-events": "^1.1.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2017": "^6.24.1",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "nodemon": "^1.17.4",
    "serve-favicon": "^2.5.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.0.10",
    "vue-server-renderer": "^2.5.16",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.16",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  }
}
